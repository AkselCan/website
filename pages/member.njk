---
layout: 'layouts/page.html'
title: 'Team Member'
pagination: 
    data: team
    size: 1
    alias: member
permalink: "/team/{{ member.name | slugify }}"
---

<li class="bio-row">
    <a href="/team/">Go back</a>
    <img src="{{ member.image }}" alt="">
    <div class="bio">
        <h3>
            {{ member.name }}
        </h3>
        <p>
            {{ member.bio | safe }}
        </p>
    </div>
</li>